{
    "type": "minecraft:multi_action",
    "title": [
        "GUI Maker v2.0 - Edit Page Name"
    ],
    "body": {
        "type": "minecraft:plain_message",
        "contents": {
            "text": "Do not leave Inputs Empty."
        }
    },
    "inputs": [
        {
            "type": "minecraft:text",
            "key": "name",
            "label": {
                "text": "Set Page Name"
            },
            "label_visible": true,
            "max_length": 9999999
        }
    ],
    "can_close_with_escape": true,
    "pause": false,
    "columns": 9,
    "actions": [
        {
            "label": "Apply",
            "width": 75,
            "action": {
                "type": "minecraft:dynamic/run_command",
                "template": "function gm:zprivate/dialog/change_page_name/main {name:'$(name)'}"
            }
        }
    ]
}